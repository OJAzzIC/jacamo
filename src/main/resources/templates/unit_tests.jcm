// project used to run the test manager agent


mas unit_tests {

    agent test_manager {
          goals:   create_tester_agents("./src/test/agt",".*.asl")
          beliefs: shutdown_delay(600)
    }

    asl-path: "."
              "src/agt"
              "src/agt/inc"
              "src/test/jacamo/inc"
              "$jacamoJar/test/jacamo/inc"
              "$jasonJar/test/jason/inc"
}
